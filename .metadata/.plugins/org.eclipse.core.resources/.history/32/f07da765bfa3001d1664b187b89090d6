package my_project_1;

public class welcome_screen {

		void get_w() {
			int w[][]= {{1,0,0,0,1},{1,0,0,0,1}};
			for(int i=0;i<5;i++) {
				for(int j=0;j<5;j++) {
					if(w[i][j]==1) 
						System.out.println("*");
						else {
							System.out.println(" ");
				}
					
			}
					System.out.println();
		}
		
}
}
